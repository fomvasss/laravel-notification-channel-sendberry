{
    "name": "fomvasss/laravel-notification-channel-sendberry",
    "description": "This package makes it easy to send notifications using [sendberry.com](https://sendberry.com) with Laravel 9.0+",
    "homepage": "https://github.com/fomvasss/laravel-notification-channel-sendberry",
    "keywords": ["laravel", "notifications", "sms", "sendberry"],
    "license": "MIT",
    "authors": [
        {
            "name": "Fomin Vasil",
            "email": "fomvasss@gmail.com",
            "homepage": "https://github.com/fomvasss",
            "role": "Developer"
        }
    ],
    "require": {
        "php": ">=8.0",
        "illuminate/notifications": "^9|^10",
        "illuminate/support": "^9|^10"
    },
    "require-dev": {
        "phpunit/phpunit": "^8.0",
        "mockery/mockery": "^1.2"
    },
    "autoload": {
        "psr-4": {
            "NotificationChannels\\Sendberry\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "NotificationChannels\\Sendberry\\Test\\": "tests"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit"
    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "providers": [
                "NotificationChannels\\Sendberry\\SendberryServiceProvider"
            ]
        }
    }
}
